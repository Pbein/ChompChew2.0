{
  "name": "chompchew",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --config tests/setup/vitest.config.ts",
    "test:unit": "vitest --config tests/setup/vitest.config.ts --run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:watch": "vitest --config tests/setup/vitest.config.ts --watch",
    "test:ui": "vitest --config tests/setup/vitest.config.ts --ui",
    "test:coverage": "vitest run --coverage --config tests/setup/vitest.config.ts",
    "test:health": "node scripts/test-maintenance.js health",
    "test:critical": "node scripts/test-maintenance.js critical",
    "test:security": "node scripts/test-maintenance.js security",
    "test:performance": "vitest run --config tests/setup/vitest.config.ts tests/performance/",
    "test:integration": "node scripts/test-maintenance.js integration",
    "test:infrastructure": "node scripts/test-maintenance.js infrastructure",
    "test:comprehensive": "node scripts/test-maintenance.js all",
    "test:clean": "node scripts/test-maintenance.js clean",
    "test:report": "node scripts/test-maintenance.js report",
    "test:benchmark": "node scripts/test-maintenance.js benchmark",
    "test:audit": "node scripts/test-quality-audit.js",
    "test:property": "vitest run --config tests/setup/vitest.config.ts tests/property/",
    "test:contract": "vitest run --config tests/setup/vitest.config.ts tests/contract/",
    "test:mutation": "stryker run",
    "test:visual": "chromatic --exit-zero-on-changes",
    "test:chaos": "vitest run --config tests/setup/vitest.config.ts tests/chaos/",
    "test:integration-real": "vitest run --config tests/setup/vitest.config.ts tests/integration-real/",
    "test:accessibility": "vitest run --config tests/setup/vitest.config.ts tests/accessibility/",
    "test:all-enhanced": "npm run test:property && npm run test:contract && npm run test:performance && npm run test:chaos",
    "playwright": "playwright test",
    "seed": "node scripts/load-seed-data.js",
    "build:all-ts-errors": "cross-env __NEXT_TEST_MODE=true npm run build",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@auth/supabase-adapter": "^1.9.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@upstash/redis": "^1.35.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "isomorphic-dompurify": "^2.25.0",
    "lucide-react": "^0.515.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "openai": "^5.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.64",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.3",
    "autoprefixer": "^10.4.21",
    "c8": "^10.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^10.1.5",
    "jsdom": "^23.0.1",
    "msw": "^2.10.2",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vitest": "^1.0.4",
    "vitest-mock-extended": "^3.1.0",
    "fast-check": "^3.15.0",
    "@stryker-mutator/core": "^8.0.0",
    "@stryker-mutator/vitest-runner": "^8.0.0",
    "@stryker-mutator/typescript-checker": "^8.0.0",
    "chromatic": "^10.0.0",
    "@testcontainers/postgresql": "^10.5.0",
    "pact": "^12.1.0",
    "lighthouse": "^11.4.0",
    "axe-playwright": "^2.0.1",
    "faker": "^6.6.6"
  }
}
